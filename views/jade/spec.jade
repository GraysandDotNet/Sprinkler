extends layout

block content
	h2 #{title}
	
	.container
		ul.nav.nav-pills
			each spec, i in specs
				li
					a(href='##{i}', data-toggle='pill') #{spec.title}
		.tab-content
				each spec, i in specs
					<div class='tab-pane fade' id=#{i}> 
					<div class='col-md-4'> 
					p
					pre #{spec.diagram} 
					</div>
					<div class='col-md-8'> 
					img(src='#{spec.wsdFilename}') 
					</div>
					</div>
